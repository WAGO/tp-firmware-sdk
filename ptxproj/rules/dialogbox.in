## SECTION=elrest_specific_applications

config DIALOGBOX
	tristate
	prompt "dialogbox"
	select QT5 if DIALOGBOX_USE_QT5
	help
	  qt based dialogbox util

if DIALOGBOX
choice
	prompt "QT_VERSION_USED"
		config DIALOGBOX_USE_QT5
			bool
			prompt "QT5"

		config DIALOGBOX_USE_QT6
			bool
			prompt "QT6"
endchoice
endif
